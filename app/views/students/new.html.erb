<h1>Add new Student</h1>
<h3><%= link_to 'Back to Student List', students_path %></h3>

<%= form_for @student do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %> <br>
  <%= f.label :full_name %>
  <%= f.text_field :full_name %> <br>
  <%= f.label :phone %>
  <%= f.text_field :phone %> <br>
  <%= f.label :hero %>
  <%= f.text_field :hero %> <br>

  <%= f.submit %>

<% end %>