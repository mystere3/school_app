<%= form_for @student do |f| %>

  <%= f.label :email, "Email Address " %>
  <%= f.text_field :email %> <br>

  <%= f.label :full_name, "Name "%>
  <%= f.text_field :full_name %> <br>

  <%= f.label :phone, "Phone Number " %>
  <%= f.text_field :phone %> <br>

  <%= f.label :hero %>
  <%= f.text_field :hero %> <br>

  <%= f.submit %>

<% end %>